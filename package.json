{
  "name": "stylight",
  "version": "0.5.7rev2",
  "description": "A CSS-In-JS module that's been made as simple as we probably wanted",
  "main": "./dist/src/index.js",
  "scripts": {
    "build": "npx webpack --config ./webpack.config.js --mode=production && npx tsc",
    "bundle-dev": "npx webpack --config ./webpack.config.js --mode=development",
    "watch": "npx tsc --watch & npx webpack --config ./webpack.config.js --mode=development --watch",
    "lint": "npx eslint --ext ts",
    "clean": "rm -rf ./dist && rm -rf ./umd",
    "prep": "npm run clean && npm i && npm run build && npm run bundle-dev",
    "test:browser": "npx karma start test/karma.conf.js",
    "test:node": "npx mocha -r source-map-support/register -r module-alias/register ./dist/test/{unit,integration}/*.js --no-deprecation",
    "test": "npm run build && npm run test:node && npm run test:browser"
  },
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": "./dist/src/index.js",
    "./*": null,
    "./dist": null,
    "./umd": null,
    "./react": "./dist/src/react/index.js"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/src/index.d.ts"
      ],
      "react": [
        "./dist/src/react/index.d.ts"
      ],
      "./*": []
    }
  },
  "keywords": [
    "css-in-js",
    "react",
    "css",
    "styles"
  ],
  "author": "Zelbov",
  "license": "MIT",
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/zelbov/stylight/issues"
  },
  "homepage": "https://github.com/zelbov/stylight#readme",
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-helmet": "^6.1.6",
    "@types/webpack-env": "^1.18.5",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "chai": "^4.3.7",
    "eslint-plugin-react": "^7.31.11",
    "eslint-webpack-plugin": "^3.2.0",
    "karma": "^6.4.3",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-mocha": "^2.0.1",
    "karma-source-map-support": "^1.4.0",
    "karma-sourcemap-loader": "^0.4.0",
    "karma-spec-reporter": "^0.0.36",
    "mocha": "^10.1.0",
    "module-alias": "^2.2.2",
    "prettier": "^2.8.1",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "source-map-loader": "^5.0.0",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.4.2",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^4.9.3",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "csstype": "^3.1.1"
  }
}
